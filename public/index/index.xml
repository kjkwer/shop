</Fonts>
    <Cols>
        <Col name="riqi" 	width="120"	align="center" sortAble="true" totalExpress="='合 计'" >日期</Col>
        <Col name="djbh"	width="80" align="center" isHyperlink='true'>单据编号</Col>
        <Col name="diancang" width="85" align="center" >店仓</Col>
        <Col name="rktype"	width="85"	align="center" >入库类型</Col>
        <Col name="gys"		width="85"	align="center" >供应商</Col>
        <Col name="pingpai" width="85"	align="center" >品牌</Col>
        <Col name="niandu"  width="45" align="center" >年度</Col>
        <Col name="jijie"  	width="45"	align="center" >季节</Col>
        <Col name="boduan"  width="45"	align="center" >波段</Col>
        <Col name="xilie" 	width="85" align="center" >系列</Col>
        <Col name="spdl"  	width="70"	align="center" >商品大类</Col>
        <Col name="spxl"  	width="70" align="center" >商品小类</Col>
        <Col name="pinming" width="85" align="center" >品名</Col>
