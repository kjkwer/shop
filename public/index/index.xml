        <Col name="chima"   width="40"	align="center" >尺码</Col>
        <Col name="tm"   	width="100"	align="center" >条码/国际码</Col>
        <Col name="dpj"  	width="70"	align="right"  displayMask="=if(data==0, '', data)"	datatype="double">吊牌价</Col>
        <Col name="jj"  	width="70"	align="right"  displayMask="=if(data==0, '', data)"	datatype="double">进价</Col>
        <Col name="shuliang" width="70" align="right"  displayMask="=if(data==0, '', data)"	datatype="int"	totalExpress="=@sum">数量</Col>
        <Col name="dpjje"  	width="85"	align="right"  displayMask="=if(data==0, '', data)"	datatype="double"	totalExpress="=@sum">吊牌金额</Col>
        <Col name="jjje"  	width="85"	align="right"  displayMask="=if(data==0, '', data)"	datatype="double"	totalExpress="=@sum">进价金额</Col>
        <Col name="beizhu"  width="85"	align="center" >备注</Col>
    </Cols>
</TreeList>